{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "es2015",
        "stage-0",
        "airbnb"
      ],
      "plugins": [
